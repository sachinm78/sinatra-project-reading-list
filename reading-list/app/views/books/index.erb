<h1>Books Index</h1>
<br />
<li>Go to: <a href="/books/show">Your Reading List</li></a>
<br />
<% if @books != nil%>
<% @books.each do |book| %>
  <%= book.title %><br />
  <%= book.author %> <br />
  <%= book.genre %> <br />
  <a href="/books/<%=book.id%>/edit">Edit</a>
  <br />
  <br />
<% end %>
<% end %>
